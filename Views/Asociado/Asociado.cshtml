@model jbchorg.Models.Asociado

@{
    ViewData["Title"] = "Contacto";
}

<div class="container">
    <div class="px-3 py-3 pb-md-4 mx-auto text-center">
<h1>Asociado</h1>
<h3>Complete el siguiente formulario</h3>
    </div>
    <div class="row">
    <div class="col-md-8 offset-md-2">    
<h3><label>Formulario de Asociado</label></h3>
</div>
    </div>
    <hr/>
@Html.ValidationSummary()

    <div class="row">

          <div class="col-md-10">

            <form asp-action="Asociado" >

           <div class="col-md-4">
               <div class="form-group"><label>Nombre:</label>
               <input asp-for="Nombre" class="form-control" />
               </div>
           </div>
           <div class="col-md-4">
               <div class="form-group"><label>Apellido Paterno:</label>
               <input asp-for="APaterno" class="form-control" />
               </div>
           </div>
           <div class="col-md-4">
               <div class="form-group"><label>Apellido Materno:</label><small>(Opcional)</small>
               <input asp-for="AMaterno" class="form-control" />
               </div>
           </div> 
           <div class="col-md-8">
               <div class="form-group"><label>Email:</label>
               <input asp-for="Correo" class="form-control" />
               </div>
           </div>
            <div class="col-md-4">
               <div class="form-group"><label>Tel√©fono de contacto:</label>
               <input asp-for="Movil" class="form-control" />
               </div>
           </div>
           <div class="col-md-4">
               <div class="form-group"><label>Tipo de Asociado:</label>
               <input asp-for="TipoAsociado" class="form-control" />
               </div>
           </div>
           <div>
           </div>
            <div class="col-md-4">
           <div class="form-group"><label for="descripcion">Grado Academico:</label>
           <input asp-for="GAcademico" class="form-control" />
            </div>
            
             <div class="form-group">
        <button type="submit" class="btn btn-primary">Registar Asociado</button>
         </div>
        </div>
      </form>
      </div>
      </div>

